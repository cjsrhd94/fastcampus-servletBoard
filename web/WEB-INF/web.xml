        <filter-name>timeCheck</filter-name>
        <filter-class>com.fastcampus.controller.common.TimeCheckFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>timeCheck</filter-name>
        <url-pattern>*.do</url-pattern>
